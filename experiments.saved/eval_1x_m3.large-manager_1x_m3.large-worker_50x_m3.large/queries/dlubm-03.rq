PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX lubm: <http://o.dlubm.ddns.me/univ-bench.owl#>
PREFIX dlubm: <http://o.dlubm.ddns.me/dlubm#>

CONSTRUCT {
	dlubm:evaluation dlubm:result ?X .
} WHERE {
	?X rdf:type lubm:Publication .
	?X lubm:publicationAuthor <http://d0.u0.dlubm.ddns.me/d#AssistantProfessor0> .
}
