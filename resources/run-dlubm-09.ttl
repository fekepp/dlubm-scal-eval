@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix scal: <http://scal#> .

<>
	a scal:Run ;
	scal:program [
		a scal:Program ;
		scal:declaration <programs/this>
	] ;
	scal:program [
		a scal:Program ;
		scal:declaration <programs/owl-imports>
	] ;
	scal:program [
		a scal:Program ;
		scal:declaration <programs/owl-ld>
	] ;
	scal:program [
		a scal:Program ;
		scal:declaration <programs/dlubm-getters>
	] ;
	scal:query [
		a scal:Query ;
		scal:declaration <queries/dlubm-09> ;
		scal:sink <resources/dlubm-09>
	] ;
	scal:trigger [
		a scal:Trigger, scal:ResourceRequestedTrigger ;
		scal:uri <resources/dlubm-09> ;
		scal:method "GET"
	]
.
